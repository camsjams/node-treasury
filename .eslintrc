{
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"env": {
		"node": true,
		"browser": true,
		"builtin": true,
		"mocha": true,
		"es6": true
	},
	"globals": {
		"_": true,
		"console": true,
		"fetch": true,
		"require": true
	},
	"rules": {
		"block-scoped-var": 2,
		"camelcase": [
			2,
			{
				"properties": "always"
			}
		],
		"curly": [
			2,
			"all"
		],
		"dot-notation": [
			2,
			{
				"allowKeywords": true
			}
		],
		"eqeqeq": [
			2,
			"allow-null"
		],
		"guard-for-in": 2,
		"no-script-url": 0,
		"new-cap": 2,
		"no-bitwise": 0,
		"no-caller": 2,
		"no-cond-assign": [
			2,
			"except-parens"
		],
		"no-debugger": 2,
		"no-empty": 2,
		"no-eval": 2,
		"no-extend-native": 2,
		"no-extra-parens": 2,
		"no-irregular-whitespace": 2,
		"no-iterator": 2,
		"no-loop-func": 2,
		"no-multi-str": 2,
		"no-new": 2,
		"no-plusplus": 0,
		"no-proto": 2,
		"no-sequences": 2,
		"no-shadow": 2,
		"no-undef": 2,
		"no-unused-vars": 2,
		"no-with": 2,
		"semi": [
			2,
			"always"
		],
		"valid-typeof": 2,
		"wrap-iife": [
			2,
			"inside"
		],
		"no-use-before-define": [
			2,
			{
				"functions": false
			}
		],
		"no-unused-expressions": 2,
		"complexity": [
			2,
			10
		],
		"max-depth": [
			2,
			4
		],
		"max-params": [
			2,
			5
		],
		"max-statements": [
			2,
			15
		],
		"space-before-function-paren": [
			2,
			{
				"anonymous": "ignore",
				"named": "never"
			}
		],
		"no-spaced-func": 2,
		"array-bracket-spacing": [
			2,
			"never",
			{}
		],
		"space-in-parens": [
			2,
			"never"
		],
		"quote-props": [
			2,
			"as-needed"
		],
		"key-spacing": [
			2,
			{
				"beforeColon": false,
				"afterColon": true
			}
		],
		"space-unary-ops": [
			2,
			{
				"words": false,
				"nonwords": false
			}
		],
		"no-mixed-spaces-and-tabs": 2,
		"comma-dangle": [
			2,
			"never"
		],
		"no-trailing-spaces": 2,
		"yoda": [
			2,
			"never"
		],
		"brace-style": [
			2,
			"1tbs",
			{
				"allowSingleLine": true
			}
		],
		"no-multiple-empty-lines": 2,
		"one-var": [
			2,
			"never"
		],
		"comma-spacing": [
			2,
			{
				"before": false,
				"after": true
			}
		],
		"semi-spacing": [
			2,
			{
				"before": false,
				"after": true
			}
		],
		"space-before-blocks": [
			2,
			"always"
		],
		"comma-style": [
			2,
			"last"
		],
		"space-infix-ops": 2,
		"eol-last": 2,
		"keyword-spacing": [
			2,
			{}
		],
		"lines-around-comment": [
			2,
			{
				"beforeLineComment": true,
				"allowBlockStart": true
			}
		],
		"max-len": [
			2,
			120
		],
		"consistent-this": [
			2,
			"self"
		],
		"linebreak-style": [
			2,
			"unix"
		],
		"quotes": [
			2,
			"single",
			{
				"avoidEscape": true
			}
		],
		"indent": [
			2,
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"prefer-arrow-callback": 2,
		"arrow-parens": 2
	}
}

{
    "preset": "node-style-guide",
    "validateIndentation": 4,
    "requireTrailingComma": false,
    "requireCapitalizedComments": false,
    "maximumLineLength": 121,
    "safeContextKeyword": [
        "self"
    ],
    "excludeFiles": [
        "node_modules"
    ],
    "requireCurlyBraces": [
        "if",
        "else",
        "for",
        "while",
        "do",
        "try",
        "catch"
    ]
}